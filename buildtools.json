{
  "$schema": "https://mobilebuildtools.com/schemas/v2/buildtools.schema.json",
  "projectSecrets": {
    "POC15": {
      "properties": [
        {
          "name": "androidAppCenterSecret",
          "type": "String",
          "defaultValue": "00000000-0000-0000-0000-000000000000"
        },
        {
          "name": "androidRedirectUriSecret",
          "type": "String",
          "defaultValue": "msauth://PACKAGE-NAME/HASH"
        },
        {
          "name": "clientSecret",
          "type": "String",
          "defaultValue": "00000000-0000-0000-0000-000000000000"
        },
        {
          "name": "iOSAppCenterSecret",
          "type": "String",
          "defaultValue": "00000000-0000-0000-0000-000000000000"
        },
        {
          "name": "iOSRedirectUriSecret",
          "type": "String",
          "defaultValue": "msauth.BUNDLE-ID://auth"
        }
      ]
    }
  }
}